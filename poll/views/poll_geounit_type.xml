<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record id="geounit_type_action_form" model="ir.actions.act_window">
    <field name="name">Location Unit types</field>
    <field name="res_model">poll.geounit.type</field>
    <field name="view_mode">tree</field>
    <field name="help" type="html">
      <p class="oe_view_nocontent_create">
        Location Units Types
      </p>
      <p>
        Here you can define types for location unit.
      </p>
    </field>
  </record>

  <record id="geounit_type_view_tree" model="ir.ui.view">
    <field name="name">Location Unit Types List</field>
    <field name="model">poll.geounit.type</field>
    <field name="arch" type="xml">
      <tree string="Location Unit Types" editable="bottom">
        <field name="name"/>
        <field name="parent_id"/>
        <field name="child_id"/>
        <field name="hierarchy"/>
      </tree>
    </field>
  </record>

</odoo>
